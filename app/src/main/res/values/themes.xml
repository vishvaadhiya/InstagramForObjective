<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.InstagramForObjective" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:statusBarColor">@color/sky</item>
        <item name="android:colorBackground">@color/sky</item>
        <item name="android:windowBackground">@drawable/ic_background</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <!--<style name="Theme.InstagramForObjective" parent="Base.Theme.InstagramForObjective" />-->
    <style name="RoundedButton" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="background">@color/sky</item>
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>

    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:navigationBarColor">@color/transparent</item>
        <item name="android:windowBackground">@drawable/ic_background</item>
    </style>

</resources>